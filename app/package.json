{
  "name": "charcuterie",
  "version": "0.1.0",
  "description": "Charcuterie is a Smartpiggies global market explorer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartpiggies/ETHNewYorkSubmission.git"
  },
  "author": "smartpiggies",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartpiggies/ETHNewYorkSubmission/issues"
  },
  "homepage": "https://github.com/smartpiggies/ETHNewYorkSubmission#readme",
  "scripts": {
    "sass:compile": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/assets/scss/ -o public/css/",
    "sass:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "build:css": "npm-run-all sass:*",
    "build:js": "react-scripts build",
    "watch:css": "chokidar \"src/assets/scss/**/*\" -c \"npm run build:css\"",
    "watch:js": "react-scripts start",
    "start": "npm-run-all -p build:css watch:*",
    "build": "npm-run-all -p build:*"
  },
  "eslintConfig": {
    "extends": "react-app",
    "parserOptions": {
      "ecmaVersion": 2017
    }
  },
  "browserslist": [
    "> 0.25%",
    "ie >= 11",
    "not op_mini all",
    "not dead"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.17",
    "@fortawesome/free-brands-svg-icons": "5.8.1",
    "@fortawesome/free-regular-svg-icons": "5.8.1",
    "@fortawesome/free-solid-svg-icons": "5.8.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "apexcharts": "3.6.5",
    "apollo-boost": "^0.3.1",
    "availity-reactstrap-validation": "2.5.1",
    "bignumber.js": "^8.1.1",
    "bootstrap": "4.3.1",
    "chart.js": "2.8.0",
    "chokidar-cli": "1.2.2",
    "classnames": "2.2.6",
    "fullcalendar": "3.10.0",
    "google-map-react": "1.1.4",
    "graphql": "^14.3.0",
    "jquery": "3.3.1",
    "jvectormap-next": "3.0.0",
    "moment": "2.24.0",
    "node-sass": "4.11.0",
    "node-sass-chokidar": "1.3.4",
    "npm-run-all": "4.1.5",
    "postcss-cli": "6.1.2",
    "react": "16.8.6",
    "react-apexcharts": "1.3.2",
    "react-apollo": "^2.5.5",
    "react-bootstrap-table-next": "3.1.0",
    "react-bootstrap-table2-paginator": "2.0.5",
    "react-bootstrap-table2-toolkit": "1.4.0",
    "react-chartjs-2": "2.7.6",
    "react-datetime": "2.16.3",
    "react-dom": "16.8.6",
    "react-dragula": "1.1.17",
    "react-feather": "1.1.6",
    "react-input-mask": "2.0.4",
    "react-perfect-scrollbar": "1.4.4",
    "react-popper": "1.3.3",
    "react-quill": "1.3.3",
    "react-redux": "6.0.1",
    "react-redux-toastr": "7.4.8",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.1.8",
    "react-select": "2.4.2",
    "react-transition-group": "2.8.0",
    "reactstrap": "7.1.0",
    "redux": "4.0.1"
  },
  "devDependencies": {}
}
